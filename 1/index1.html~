        "Anjali Rai",
    "Chandani Singh",
    "Aprajita Singh",
    "Sakshi Agarwal",
    "Ritika Verma",
        "Anjali Rai",    "Anjali Rai"

	];
    var counter = 0;
    var elem = document.getElementById("changeText");
    setInterval(change, 0);
    function change() {
     elem.innerHTML = text[counter];
        counter++;
        if(counter >= text.length) { counter = 0; }
    }
